      <li>CallFoodAidSmall</li>
      <li>CallFoodAidLarge</li>
      <li>CallFoodAidGrand</li>
      <li>CallMedicalAidSmall</li>
      <li>CallMedicalAidLarge</li>
      <li>CallMedicalAidGrand</li>
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>*/RoyalTitleDef[defName="Praetor" or defName="Baron"]/permits</xpath>
    <value>
      <li>CallSiegeAidSmall</li>
    </value>
