      <li>CallFoodAidLarge</li>
      <li>CallMedicalAidSmall</li>
      <li>CallMedicalAidLarge</li>
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>*/RoyalTitleDef[defName="Baron" or defName="Count"]/permits</xpath>
