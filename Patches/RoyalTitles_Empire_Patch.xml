        <permits>
          <li>CallFoodAidSmall</li>
          <li>CallFoodAidLarge</li>
          <li>CallFoodAidGrand</li>
          <li>CallMedicalAidSmall</li>
          <li>CallMedicalAidLarge</li>
          <li>CallMedicalAidGrand</li>
          <li>CallSiegeAidLarge</li>
        </permits>
      </value>
  </Operation>
