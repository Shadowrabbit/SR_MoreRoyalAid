    <value>
      <li>CallFoodAidSmall</li>
      <li>CallMedicalAidSmall</li>
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>*/RoyalTitleDef[defName="Praetor"]/permits</xpath>
